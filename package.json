{
  "name": "tweenkey",
  "version": "0.0.6",
  "description": "A compact tweening engine for Javascript.",
  "main": "src/tweenkey.js",
  "types": "types/tweenkey.d.ts",
  "scripts": {
    "test": "mocha-phantomjs tests/test-runner.html",
    "test-style": "jscs src/tweenkey.js",
    "build:uglify": "uglifyjs dist/tweenkey.js -c -m --reserved-file=min.reserved --mangle-props -o dist/tweenkey.min.js",
    "build:merge": "concat-cli -f src/intro.js src/easing.js src/tweenkey.js src/outro.js -o dist/tweenkey.js",
    "build": "npm run build:merge & npm run build:uglify",
    "watch": "node node_modules/watch-cli/bin/watch -p \"src/*.js\" -c \"npm run build:merge\"" ,
    "start": "concurrently \"npm run watch\" \"node node_modules/http-server/bin/http-server -c-1\" "
  },
  "repository": {
    "type": "git",
    "url": "https://radixzz@github.com/radixzz/Tweenkey.git"
  },
  "keywords": [
    "Animation",
    "tween",
    "timeline",
    "interpolation",
    "motion",
    "performance"
  ],
  "author": "Iván Juárez Núñez",
  "license": "MIT",
  "bugs": {
    "url": "https://git hub.com/radixzz/Tweenkey/issues"
  },
  "homepage": "https://github.com/radixzz/Tweenkey",
  "devDependencies": {
    "chai": "^3.5.0",
    "concat-cli": "^4.0.0",
    "concurrently": "^3.3.0",
    "http-server": "^0.9.0",
    "jscs": "^2.9.0",
    "mocha": "^2.4.5",
    "mocha-phantomjs": "^4.0.2",
    "uglify-js": "^2.6.1",
    "watch-cli": "^0.2.1"
  }
}
